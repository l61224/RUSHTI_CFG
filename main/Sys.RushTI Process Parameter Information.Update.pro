#Region - Header
##################################################################################################################
# PURPOSE:
#    Generate TI list then call TI: 'Sys.RushTI Process Parameter Information.ReadProcess'
#
# DATA SOURCE:
#     Process List file generated by Command
#
# REMARK:
#   This process will call TI: 'Sys.RushTI Process Parameter Information.Delete.File' @Epilog to delete Process List file
#   
# CHANGE HISTORY:
#   DATE          CHANGED BY          COMMENT
#   2025-09-14    Jacky Lai           Create Process
##############################################################################################################
#EndRegion - Header
#****Begin: Generated Statements***
#****End: Generated Statements****

##############################################################################################################
#Region - Declare Constants
# Standard naming convention for source and target cubes/dimensions
#~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
# Variables
#~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
cCubSysParam    = 'Sys RushTI Parameter';
cDimRushTIProc  = 'Sys RushTI Process';
nPosition       = 0;
sThisProcName   = GetProcessName();
cExportPath     = CellGetS( cCubSysParam, 'RushTI Task Export Folder', 'Text');

If( SubSt( cExportPath, Long( cExportPath), 1 ) @= '\' 
    % SubSt( cExportPath, Long( cExportPath), 1 ) @= '/' );
  cExportPath = SubSt( cExportPath, 1, Long(cExportPath)-1 );
EndIf;

## Parameter
cInstance       = pInstance;
cLineItem       = pLineItem;

## String
cDataDirectory  = CellGetS( 'Sys RushTI Instance Information', 'Default Instance', cLineItem, 'Data Folder Path');
If( cDataDirectory @= '');
  DataSourceType = 'NULL';
  sErr = 'Data Folder Path CANNOT be blank. Please check.';
  ItemReject( sErr);
EndIf;

#EndRegion - Declare Constants
##############################################################################################################

##############################################################################################################
#Region - Create Data Source

If( SubSt( cDataDirectory, Long( cDataDirectory), 1 ) @= '\' 
    % SubSt( cDataDirectory, Long( cDataDirectory), 1 ) @= '/' );
  cDataDirectory = SubSt( cDataDirectory, 1, Long(cDataDirectory)-1 );
EndIf;

## If Subset not exist, CREATE
cDimRushTIProc = 'Sys RushTI Process';
If( SubsetExists( cDimRushTIProc, cInstance) = 0);
  SubsetCreate( cDimRushTIProc, cInstance, 0);
EndIf;

## If Subset exist, Delete All Elements
If( SubsetExists( cDimRushTIProc, cInstance) <> 0);
  SubsetDeleteAllElements( cDimRushTIProc, cInstance);
EndIf;

## Generate Process List File
cProcessListFile = cExportPath | '\' | cInstance | '_rushtiprocesslist.txt';
ExecuteCommand('cmd /C dir /b "' | cDataDirectory | '\*.pro*" | findstr /i "\.pro$" > ' | cProcessListFile, 1);

DataSourceType ='CHARACTERDELIMITED';
DatasourceNameForServer = cProcessListFile;
DatasourceASCIIHeaderRecords=0;

#EndRegion - Create Data Source
##############################################################################################################
 
