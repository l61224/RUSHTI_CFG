#****Begin: Generated Statements***
#****End: Generated Statements****
574,45
#****Begin: Generated Statements***
#****End: Generated Statements****

##############################################################################################################
#Region - Definition
sProcess = vProcessName;

#EndRegion - Definition
##############################################################################################################

#############################################################################################################
# Region - Add 'Sys RushTI Processes' Subset content
If( ELLEV( cDimRushTIProc, sProcess ) = 0 );
  If( ( SubSt( sProcess, 1, 7 ) @= 'Bedrock' % SubSt( sProcess, 1, 8 ) @= '}bedrock' ) % ( SubSt( sProcess, 1, 4 ) @= '}APQ' & SubSt( sProcess, 1, 7 ) @<> '}APQ.C3' ) );
    sProcessPath = cDataDirectory | '\' | sProcess;
  Else;
    sProcessPath = cDataDirectory | '\' | sProcess;
  EndIf;
  
  sResult = FileExists( sProcessPath );
  If( sResult = 1 );
    sTIName = SubSt( sProcess, 1, Long( sProcess) - Long( '.pro'));
    ## Add 'Sys RushTI Processes' content
    If( DimIx( cDimRushTIProc, sTIName) = 0);
      DimensionElementInsertDirect( cDimRushTIProc, '', sTIName, 'N');
    EndIf; 
     
    ## Add 'Sys RushTI Processes' subset content
    If( SubsetElementExists( cDimRushTIProc, cInstance, sTIName) = 0);
      nPosition = nPosition + 1;
      SubsetElementInsert( cDimRushTIProc, cInstance, sTIName, nPosition);
    EndIf;
    
    ## Executete Read Process TI
    ExecuteProcess( 'Sys.RushTI Process Parameter Information.ReadProcess', 
                    'pInstance'   , cInstance, 
                    'pProcessPath', sProcessPath, 
                    'pProcess'    , sTIName
                  );
                  
  EndIf;
EndIf;

# Region - Add 'Sys RushTI Processes' Subset content
#############################################################################################################
